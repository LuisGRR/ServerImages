    <%- include("partials/head.ejs") %>
  <body>
    <%- include("partials/navbar.ejs") %>

    <div class="pl-16 lg:pl-64 h-screen w-full">
      <div class="flex flex-wrap justify-between">
        <div class="col-md-4 offset-md-4">
          <table class="border-collapse table-fixed w-full text-md text-left ">
            <thead class="uppercase text-xs bg-don-juan-800 text-detailColor">
              <tr>
                <th
                  class="border-b font-medium px-6 py-3"
                >
                  title
                </th>
                <th
                  class="border-b font-medium px-6 py-3"
                >
                  description
                </th>
                <th
                  class="border-b font-medium px-6 py-3"
                >
                  filename
                </th>
                <th
                  class="border-b font-medium px-6 py-3"
                >
                  path
                </th>
                <th
                  class="border-b font-medium px-6 py-3"
                >
                  Rezise
                </th>
              </tr>
            </thead>
            <tbody >
              <% images.forEach(function(image){ %>
              <tr class="bg-detailColor border-b even:bg-don-juan-600  hover:bg-don-juan-400 odd:bg-don-juan-500">
                <td
                  class="px-6 py-4 text-detailColor truncate whitespace-nowrap"
                >
                  <%= image.title %>
                </td>

                <td
                  class="px-6 py-4 text-detailColor truncate whitespace-nowrap"
                >
                  <%= image.description %>
                </td>

                <td
                  class="px-6 py-4 text-detailColor  whitespace-nowrap truncate"
                >
                  <%= image.filename %>
                </td>

                <td
                  class="px-6 py-4 text-detailColor  whitespace-nowrap truncate"
                >
                  <%= image.path %>
                </td>
                <td
                  class="px-6 py-4 text-detailColor truncate whitespace-nowrap"
                >
                  <a
                    href="/image/<%= image._id%>/rezise"
                    class="w-full text-center font-bold"
                    >Rezise</a
                  >
                </td>
              </tr>

              <% }); %>
            </tbody>
          </table>
        </div>
      </div>
      <%- include("partials/footerCss.ejs") %>
    </div>
  </body>
</html>
