<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>ServerImage</title>
    <%- include("partials/head.ejs") %>
  </head>
  <body>
    <%- include("partials/navbar.ejs") %>
    <div class="container p-4">
      <div class="row" data-masonry='{"percentPosition": true }'>
        <% images.forEach(function(images){ %>
        <div class="col-sm-6 col-lg-4 mb-4">
          <div class="card">
            <img
              src="<%= images.path %>"
              alt="images.title"
              class="card-img-top"
            />
            <div class="card-body">
              <h5 class="card-title"><%= images.title %></h5>
              <p class="card-text"><%= images.description %></p>
            </div>
            <div class="card-footer">
              <p class="card-text"><%= format(images.created_at) %></p>
              <a href="/image/<%= images._id %>" class="btn btn-primary"
                >View Peofile</a
              >
            </div>
          </div>
        </div>

        <% }) %>
      </div>
    </div>

    <%- include("partials/footerCss.ejs") %>
  </body>
</html>
