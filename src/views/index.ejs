<%- include("partials/head.ejs") %>
  <body>
    <%- include("partials/navbar.ejs") %>
    <div class="pl-16 lg:pl-64 h-screen w-full">
      <div class="gap-8 columns-4 m-0 p-4">
        <% images.forEach(function(image){ %>
        <div
          class="mb-4 bg-white shadow-lg rounded-xl overflow-hidden"
        >
          <a href="/image/<%= image._id %>" class="my-3 rounded-xl w-full object-cover">
            <img
              src="<%= image.path %>"
              alt="image.title"
              class=""
            />
            <!--div class="text-center font-medium text-lg py-4 w-[calc(33.33% - 30px)]">
              <h5 class=""><%= //image.title %></h5>
              <p class=""><%= //image.description %></p>
              <p class=""><%= //format(image.created_at) %></p>
            </div-->
          </a>
        </div>

        <% }); %>
      </div>

      <%-// include("partials/footerCss.ejs") %>
    </div>
  </body>
</html>
